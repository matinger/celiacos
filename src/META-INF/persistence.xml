<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
	version="2.0">

	<persistence-unit name="celiacos">
		<provider>org.hibernate.ejb.HibernatePersistence</provider>
		<class>Celiacos.Cuota</class>
		<class>Celiacos.Notificacion</class>
		<class>Celiacos.Pago</class>
		<class>Celiacos.Usuario</class>
		<class>Celiacos.TipoUnidad</class>
		<class>Celiacos.Cuota</class>
		<class>Celiacos.Perfil</class>
		<class>Celiacos.PerfilOperadorCentral</class>
		<class>Celiacos.PerfilOperadorUnidad</class>
		<class>Celiacos.PerfilAdministradorCentral</class>
		<class>Celiacos.PerfilAdministradorUnidad</class>
		<class>Celiacos.PerfilSocio</class>
		<class>Celiacos.Producto</class>
		<class>Celiacos.Restaurante</class>
		<class>Celiacos.Unidad</class>
		<class>Celiacos.UnidadCentral</class>
		<properties>
			<property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver" />
			<property name="hibernate.connection.username" value="root" />
			<property name="hibernate.connection.password" value="" />
			<property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/celiacos" />
			<property name="hibernate.default_schema" value="celiacos" />
			<property name="hibernate.dialect" value="org.hibernate.dialect.MySQL5InnoDBDialect" />
			<property name="hibernate.hbm2ddl.auto" value="update" />
		</properties>
	</persistence-unit>
</persistence>